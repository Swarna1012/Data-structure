#include<stdio.h>
#include<stdlib.h>

struct node
{
    int data;
    struct node *next;
    struct node *prev;
}*head,*temp,*tail,*newnode;

int main()
{
    int n;
    printf("enter no. of elements ");
    scanf("%d",&n);

    for(int i=0;i<n;i++)
    {
        newnode=malloc(sizeof(struct node));
        printf("enter the data ");
        scanf("%d",&newnode->data);
        newnode->next=NULL;
        newnode->prev=NULL;
        if(head==NULL)
        {
            head=temp=tail=newnode;
        }
        else
        {
            head->next=newnode;
            newnode->prev=temp;
            temp=tail=newnode;
            newnode->next=head;
        }
    }

        while(tail!=NULL)
        {
            printf("%d ",tail->data);
            tail=tail->prev;
        }

}
